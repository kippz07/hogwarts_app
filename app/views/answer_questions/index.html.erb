<p id="notice"><%= notice %></p>

<h1>Answer Questions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @answer_questions.each do |answer_question| %>
      <tr>
        <td><%= link_to 'Show', answer_question %></td>
        <td><%= link_to 'Edit', edit_answer_question_path(answer_question) %></td>
        <td><%= link_to 'Destroy', answer_question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Answer Question', new_answer_question_path %>
