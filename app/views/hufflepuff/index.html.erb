<div id="huff-body">
  <h1>Hufflepuff</h1>
  <div class="left">
    <h2>Members</h2>
    <% @users.each do |user| %>
      <p><%= user.name %></p>
    <% end %>
  </div>

  <div class="right">
    <% @indi_spells.each do |spell| %>
      <h2><%= spell.name %></h2>
      <h2><%= spell.catagory %></h2>
    <% end %>
  </div>
</div>